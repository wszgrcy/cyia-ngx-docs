{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@rxactions/*": [
        "./src/app/actions/*"
      ],
      "@rxreducer/*": [
        "./src/app/reducer/*"
      ],
      "@resource-entity/*": [
        "./src/app/resource-entity/*"
      ],
      "@dynamic-loading-elements/*": [
        "./src/app/dynamic-loading-elements/*"
      ],
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
}