{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@rxactions/*": [
        "./src/app/actions/*"
      ],
      "@rxreducers": [
        "./src/app/reducer/index"
      ],
      "@rxselectors/*": [
        "./src/app/selector/*"
      ],
      "@resource-entity/*": [
        "./src/app/resource-entity/*"
      ],
      "@dynamic-loading-elements/*": [
        "./src/app/dynamic-loading-elements/*"
      ],
      "@markdown-custom/*": [
        "./src/app/components/*"
      ],
      "@project-types": [
        "./src/app/types"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ],
}